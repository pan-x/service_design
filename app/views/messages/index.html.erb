<h1>Listing messages</h1>

<table>
  <tr>
    <th>Session</th>
    <th>Owner</th>
    <th>Timestamp</th>
    <th>Receiver</th>
    <th>Text</th>
    <th>Media</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @messages.each do |message| %>
  <tr>
    <td><%= message.session_id %></td>
    <td><%= message.owner_id %></td>
    <td><%= message.timestamp %></td>
    <td><%= message.receiver_id %></td>
    <td><%= message.text %></td>
    <td><%= message.media %></td>
    <td><%= message.location_id %></td>
    <td><%= link_to 'Show', message %></td>
    <td><%= link_to 'Edit', edit_message_path(message) %></td>
    <td><%= link_to 'Destroy', message, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Message', new_message_path %>
