<!DOCTYPE html>
<html>
	<head>
		<title>ServiceDesign</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<div class="w980">			
			<div id="navi">
				<% unless @current_user.blank?%>
				<% if is_admin? %>
				<%= link_to 'Profile', users_path%>
				<% else %>
				<%= link_to 'Profile', user_path(@current_user)%>
				<% end %>
				<%= link_to 'Sessions', show_active_sessions_path %>
				<%= link_to 'new Message', new_session_path %>
				<%= link_to 'logout', access_logout_path%>
				<%= yield :navi %>
				<% end %>

			</div>
			<div id="content">
			<%= yield %>
			</div>
		</div>
	</body>
</html>

<script>
	$(document).ready()
{
	getPosition();
	//window.setInterval("getPosition()",20000);
	
	
}
	
</script>
